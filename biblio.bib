  issue           = {32},
  keywords        = {Anhydrides, chemistry; Molecular Structure; Nitrobenzoates, chemistry; Protein Structure, Tertiary; RNA, chemistry; ortho-Aminobenzoates, chemistry},
  mid             = {NIHMS398353},
  nlm-id          = {7503056},
