  completed       = {2019-03-04},
  country         = {United States},
  doi             = {10.1021/acs.jpcb.8b00575},
  issn-linking    = {1520-5207},
  issue           = {18},
  keywords        = {Acylation; Hydroxyl Radical, metabolism; Molecular Dynamics Simulation; Nucleic Acid Conformation; RNA, chemistry, metabolism},
  mid             = {NIHMS1002948},
  nlm-id          = {101157530},
  owner           = {NLM},
  pmc             = {PMC6311105},
