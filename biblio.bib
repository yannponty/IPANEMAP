  issn-linking    = {0022-2836},
  issue           = {1},
  keywords        = {Base Sequence; Binding Sites; CME-Carbodiimide, analogs & derivatives, pharmacology; Diethyl Pyrocarbonate, pharmacology; Magnesium, pharmacology; Molecular Sequence Data; Nucleic Acid Conformation; Organometallic Compounds, pharmacology; RNA, Catalytic, chemistry, classification, drug effects; Structure-Activity Relationship; Sulfuric Acid Esters, pharmacology},
  nlm-id          = {2985088R},
