  keywords        = {Base Sequence; Indicators and Reagents; Macromolecular Substances; Molecular Sequence Data; Nucleic Acid Conformation; Nucleotide Mapping; Nucleotidyltransferases; Polynucleotide 5'-Hydroxyl-Kinase; RNA; RNA Probes},
  nlm-id          = {0212271},
